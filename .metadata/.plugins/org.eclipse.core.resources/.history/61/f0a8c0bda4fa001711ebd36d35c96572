# The pattern
x, y, z = [1, 2, 3]

# also works in loops:
Gunnar = [(28, 'M',"Braveman"), (4, 'a','Tull'), (1990, 'r','kom igjen')]
for x, y, z in Gunnar:
    print(z)  # prints the numbers 28, 4, 1990

# and also
for index, (x, y,z) in enumerate(Gunnar):
    print(z,x)  # prints the numbers 28, 4, 1990
    
table = {'Gunnar Andresen': 4127, 'Nikolai Andresen': 4098, 'Elisabeth Andresen': 7678}
for name, phone in table.items():
    print('{0:1d} ==> {1:10d}'.format(name, phone))
    